@article{irzBiologicalConditionFrench2024,
  title = {The Biological Condition of {{French}} Rivers Has Not Improved over the Last Seventeen Years According to the National {{Fish-Based-Index}} ({{FBI}})},
  author = {Irz, Pascal and Vigneron, Thibault and Poulet, Nicolas and Richard, Benoît and Oberdorff, Thierry},
  date = {2024},
  journaltitle = {Knowledge \& Management of Aquatic Ecosystems},
  shortjournal = {Knowl. Manag. Aquat. Ecosyst.},
  number = {425},
