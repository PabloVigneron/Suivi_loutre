@article{hongSecondaryForestDevelopment2021,
  title = {Secondary Forest Development during Urbanization Sustains Apex Carnivore Populations of {{Eurasian}} Otters ({{{\mkbibemph{Lutra}}}}{\mkbibemph{ Lutra}})},
  author = {Hong, Sungwon and Joo, Gea-Jae},
  date = {2021-04-01},
  journaltitle = {Landscape and Urban Planning},
  shortjournal = {Landscape and Urban Planning},
  volume = {208},
  pages = {104021},
  issn = {0169-2046},
  doi = {10.1016/j.landurbplan.2020.104021},
  url = {https://www.sciencedirect.com/science/article/pii/S016920462031505X},
  urldate = {2025-05-31},
  abstract = {Better understanding of the ecological and socioeconomic determinants of land-use change is a key to achieve more sustainable development and conservation objectives. Long-term studies on the ability of regenerated secondary forests to sustain biodiversity compared with primary forests have been little explored. South Korea has experienced rapid urbanization and rapid forest regeneration. In this study, we asked two questions: (1) Does secondary forest development alleviate the negative impacts of urbanization on an apex predator; and (2) Can we elucidate a land use planning balance between urbanization and biodiversity conservation using scenario analysis? We studied the Eurasian otter (Lutra lutra) population in South Korea. We applied generalized linear mixed models to five national otter surveys from 1994 to 2019 with 18 environmental variables. Based on the most parsimonious model, we developed 10 scenarios with three patterns of urbanization (dispersed, clustered, and no further urbanization) with or without forest development or with forest degeneration. The most parsimonious model showed that otter occurrences steadily increased with greater forest density, but were negatively correlated with the percentage of urban areas. The scenarios suggested that diffused urban development had a less negative impact on otter distribution than clustered urban areas. Developed forests considerably alleviated the negative impacts of urbanization. In sample sub-basins, scenarios showed that even without further urbanization, otters will take more than six years to occur with the current forest development trends. Hence, sustainable urban planning should include strategic spatial planning for forest development and preservation.},
  keywords = {Generalized linear mixed models,Scenario analysis,Secondary forest regeneration,Urban and landscape planning,Wildlife management},
  file = {C:\Users\pablo\Zotero\storage\5IJ3YG5J\S016920462031505X.html}
}

